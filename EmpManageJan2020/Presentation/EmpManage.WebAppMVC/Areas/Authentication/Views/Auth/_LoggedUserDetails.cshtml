@model EmpManage.WebAppMVC.Areas.Admin.Models.UserManagement.UserAccountViewModel

<div class="modal" id="modalLoggedUserDetails" data-backdrop="static">
    <div class="modal-dialog modal-lg" role="document">
        <div class="modal-content">

            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body" id="">

                <div class="row">
                    <div class="col-12">
                        <div class="bs-component">
                            <ul class="nav nav-tabs pl-2">
                                <li class="nav-item">
                                    <a class="nav-link active" data-toggle="tab" href="#ViewLoggedInUserDetails">
                                        <i class="fas fa-user-tie"></i>User Details
                                    </a>
                                </li>
                                <li class="nav-item">
                                    <a class="nav-link" data-toggle="tab" href="#EditLoggedInUserDetails">
                                        <i class="fas fa-user-edit"></i>  Edit Details
                                    </a>
                                </li>
                                <li class="nav-item">
                                    <a class="nav-link" data-toggle="tab" href="#LoggedInUserChangePassword">
                                        <i class="fas fa-key"></i> Change Password
                                    </a>
                                </li>
                                <li class="nav-item">
                                    <a class="nav-link" data-toggle="tab" href="#LoggedInUserDetailsHistory">
                                        <i class="fas fa-user-clock"></i> Login History
                                    </a>
                                </li>


                            </ul>
                            <div id="myTabContent" class="tab-content">
                                <div class="tab-pane fade show active" id="ViewLoggedInUserDetails">


                                </div>
                                <div class="tab-pane fade" id="EditLoggedInUserDetails">



                                </div>
                                <div class="tab-pane fade" id="LoggedInUserChangePassword">

                                </div>
                                <div class="tab-pane fade" id="LoggedInUserDetailsHistory">

                                </div>

                            </div>
                        </div>
                    </div>
                </div>
            </div>

        </div>
    </div>
</div>

<script>

    $('#btnModalLoggedUserDetailsCnl').click(function () {
        $('#modalLoggedUserDetails').modal('hide');
        $('#modalLoggedUserDetails').remove();
    });

    $(window).on("load", function () {

    });
</script>